{
	"name": "express-typeorm",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start": "node dist/index.js",
		"dev": "nodemon src/index.ts",
		"build": "tsc",
		"test": "jest --detectOpenHandles",
		"seed": "npx ts-node src/seeds/run-seeder.ts",
		"seed:clear": "npx ts-node src/seeds/run-seeder.ts clear",
		"seed:reseed": "npx ts-node src/seeds/run-seeder.ts reseed",
		"typeorm": "ts-node ./node_modules/typeorm/cli.js",
		"migration:generate": "npm run typeorm -- -d src/data-source.ts migration:generate src/migrations/SchemaSyncCheckAfterFix"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.24",
		"bcrypt": "^6.0.0",
		"bcryptjs": "^3.0.2",
		"cors": "^2.8.5",
		"express": "^4.18.3",
		"express-typeorm": "file:",
		"mysql2": "^3.9.2",
		"nodemon": "^3.1.0",
		"reflect-metadata": "^0.2.1",
		"typeorm": "^0.3.20",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.27.2",
		"@babel/preset-typescript": "^7.27.1",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^29.5.14",
		"@types/supertest": "^6.0.3",
		"babel-jest": "^30.0.0-beta.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^30.0.0-beta.3",
		"supertest": "^7.1.1",
		"ts-jest": "^29.3.4",
		"ts-node": "^10.9.2"
	}
}
